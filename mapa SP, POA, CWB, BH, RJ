   "source": [
    "import folium\n",
    "\n",
    "mapa = folium.Map(location=[latitude,longitude], zoom_start=5)\n",
    "\n",
    "# Belo Horizonte\n",
    "latitude = -19.9173 \n",
    "longitude = -43.9345\n",
    "\n",
    "folium.Marker([latitude, longitude], popup='Belo Horizonte, BH, Brasil').add_to(mapa)\n",
    "\n",
    "# Porto Alegre\n",
    "latitude = -30.0346 \n",
    "longitude = -51.2177\n",
    "\n",
    "folium.Marker([latitude, longitude], popup='Porto Alegre, RS, Brasil').add_to(mapa)\n",
    "\n",
    "# São paulo\n",
    "latitude = -23.5505\n",
    "longitude = -46.6333\n",
    "\n",
    "folium.Marker([latitude, longitude], popup='São Paulo, SP, Brasil').add_to(mapa)\n",
    "\n",
    "# Rio de janeiro\n",
    "latitude = -22.9068\n",
    "longitude = -43.1729 \n",
    "\n",
    "folium.Marker([latitude, longitude], popup='Rio de Janeiro, RJ, Brasil').add_to(mapa)\n",
    "\n",
    "# Curitiba\n",
    "latitude = -25.4290 \n",
    "longitude = -49.2671   \n",
    "\n",
    "folium.Marker([latitude, longitude], popup='Curitiba, CWB, Brasil').add_to(mapa)\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# Exibindo o mapa\n",
    "mapa"
